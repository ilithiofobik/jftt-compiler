VAR
	a, b, c[1234:1235]
BEGIN
	READ a;
	b ASSIGN 10;
	c[1234] ASSIGN 1;
	c[1235] ASSIGN c[1234] TIMES 134217727; ( c[1235] EQ 2^27 MINUS 1 )
	c[1235] ASSIGN c[1235] DIV 134217727; ( c[1235] EQ 1 )
	FOR k FROM 999999 DOWNTO 0 DO
		FOR i FROM 1 TO 3 DO
			a ASSIGN a PLUS b;
			c[1234] ASSIGN c[1234] PLUS c[1235];
		ENDFOR
	ENDFOR
	WRITE a;
	WRITE c[1234];
END
